module namespace test = "http://test.com";

declare namespace rest = "http://exquery.org/ns/restxq";

(: with default params :)
declare
    %rest:GET
    %rest:path("/test")
    %rest:query-param("hello", "{$hello}", "hello", "again")
function test:hello($hello) {
    <hello>{$hello}</hello>
};

